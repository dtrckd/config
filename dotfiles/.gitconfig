[user]
	name = dtrckd
	email = dtrckd@gmail.com
[push]
	default = simple
[branch]
	autosetupmerge =
[credential]
    helper = cache --timeout=3600000
[diff]
	tool = vimdiff
[pull]
	rebase = true
[core]
	excludesfile = /home/dtrckd/.gitignore
    pager = delta --dark --syntax-theme="Dracula" --file-style='bold 201' --plus-style='syntax 17'
[delta]
    side-by-side = true
[init]
	defaultBranch = main
