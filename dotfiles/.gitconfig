[user]
	name = dtrckd
	email = dtrckd@gmail.com
[push]
	default = simple
[branch]
	autosetupmerge =
[credential]
    helper = cache --timeout=3600000
[diff]
	tool = vimdiff
[pull]
	rebase = true
[core]
	excludesfile = /home/dtrckd/.gitignore
    #pager = delta --dark --plus-style=syntax 17 --minus-style=normal 53 --hunk-header-style=3 --syntax-theme="TwoDark" --file-style=226
    pager = delta --dark  --syntax-theme="Dracula" --file-style=226  
[delta]
    side-by-side = true
[init]
	defaultBranch = main
